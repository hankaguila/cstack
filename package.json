{
  "name": "cstack",
  "version": "0.3.3",
  "description": "Debug your code, not the Runtime",
  "keywords": [
    "callstack",
    "stacktrace",
    "debug"
  ],
  "author": "Henry Aguila",
  "license": "MIT",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/user?u=80437116"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hankaguila/cstack"
  },
  "homepage": "https://github.com/hankaguila/cstack#readme",
  "bugs": "https://github.com/hankaguila/cstack/issues",
  "files": [
    "build"
  ],
  "main": "build/index.cjs",
  "module": "build/index.mjs",
  "types": "build/index.d.ts",
  "type": "module",
  "scripts": {
    "node": "node --experimental-specifier-resolution=node --no-warnings",
    "test": "vitest --dir=test run",
    "lint": "eslint src --fix",
    "build:cjs": "tsup src/index.ts -d=build --format=cjs",
    "build:esm": "tsup src/index.ts -d=build --format=esm && shx mv build/index.js build/index.mjs",
    "build:dts": "tsup src/index.ts -d=build --dts-only --format=esm",
    "build": "shx rm -rf build && npm run build:cjs && npm run build:esm && npm run build:dts"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "8.22.0",
    "shx": "^0.3.4",
    "tsup": "^7.1.0",
    "typescript": "^4.9.5",
    "vitest": "^0.32.0"
  }
}
